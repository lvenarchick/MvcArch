<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE taglib
        PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
        "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
    <tlib-version>1.0</tlib-version>
    <jsp-version>1.2</jsp-version>
    <short-name>oas</short-name>
    <uri>oas-taglib</uri>
    <display-name>oas-taglib</display-name>
    <small-icon></small-icon>
    <large-icon></large-icon>
    <description>Common tags for OAS</description>

    <tag>
        <name>DateOfBirthTag</name>
        <tag-class>com.acs.oas.waf.view.taglibs.DateOfBirthTag</tag-class>
        <body-content>JSP</body-content>
        <description>Creates a list box that contains date of birth</description>
        <attribute>
        	<name>selected</name>
        	<required>true</required>
        	<rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
        	<name>listBoxName</name>
        	<required>true</required>
        	<rtexprvalue>true</rtexprvalue>
        </attribute>        	
    </tag> 
