  <!-- Fast Track Info Tag -->
  <tag>
    <name>FastTrackTag</name>
    <tagclass>com.acs.oas.waf.view.taglibs.FastTrackTag</tagclass>
    <bodycontent>JSP</bodycontent>
    <info>An input field that can be validated. Use with form tag.</info>
    <attribute>
      <name>bankName</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
    <attribute>
      <name>acctLast4</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>    
    <attribute>
      <name>action</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute> 
